{
  "name": "Scroll FB Timeline",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "profile_image.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["popup.js"]
    }
  ]
}
